# automatizar uma procura no Google Chrome para entrar no Youtube e acessar uma m√∫sica.
import pyautogui as pa  
pa.PAUSE = 1

pa.press('WIN')
pa.write("chrome")
pa.press('ENTER')
pa.write("youtube.com")
pa.press("enter")
pa.click(x=702, y=145)
pa.write("Chris Brown")
pa.press("enter")
pa.click(x=670, y=320)
